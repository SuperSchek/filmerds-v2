  }

  /* Navigation */
  /* Mobile */
  #sidebar {
    background: #385c80;
    color: #FFF;
    padding: 0 2rem;
  }
  #sidebar ul {
    list-style-type: none;
    height: 90vh;
    padding: 0;
    text-transform: uppercase;
    letter-spacing: .1rem;
  }
  #sidebar a {
    color: #FFF;
    text-decoration: none;
  }
  #sidebar span {
    color: #FFF;
  }

  nav { text-align: center; }

  .btn-nav {
    color: #385c80;
    text-transform: uppercase;
    position: relative;
    display: inline-block;
    border: solid 2px #385c80;
    background: white;
    margin-right: 15px;
    padding: .4rem 2rem;
    transition: all 0.15s ease-in-out;
    letter-spacing: .1rem;
    text-align: center;
    cursor: pointer;
    text-decoration: none;
  }

  .btn-nav a {
    text-decoration: none;
  }

  .btn-nav:after {
    border: solid 2px #385c80;
    width: 100%;
    height: 100%;
    content: "";
    left: 3px;
    z-index: -1;
    top: 3px;
    position: absolute;
  }

  .active:after {
    border: solid 2px #385c80;
    width: 100%;
    height: 100%;
    content: "";
    left: 3px;
    z-index: -1;
    top: 3px;
    position: absolute;
    background: #385c80;
  }

  .btn-nav:hover {
    background: #385c80;
    color: #FFF;
  }

